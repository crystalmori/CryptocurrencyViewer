<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <padding>
        <Insets bottom="25" left="25" right="25" top="25" />
    </padding>

    <children>
    <TextArea fx:id="queryTextArea"  GridPane.columnIndex="0" GridPane.columnSpan="3" GridPane.rowIndex="0" GridPane.rowSpan="1"/>

    <Button onAction="#buttonPressed" text="Go" GridPane.columnIndex="3" GridPane.rowIndex="0" />

    <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.rowIndex="1" GridPane.rowSpan="1"/>

    </children>


</GridPane>
